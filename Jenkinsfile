pipeline {
  agent any
  stages {
    stage('print text using shell script') {
      steps {
        echo 'This is a test to check if we can trigger the build with jenkins git trigger'
        echo 'testing in process..........'
        echo 'try adding git integration plugin to start triggering jenkins job with git commit'
      }
    }
    stage('read console output for instructions') {
      steps {
        echo 'try adding git integration plugin to start triggering jenkins job with git commit'
        echo 'try to add the jenkins url in webhook and then commit will trigger jenkins job'
        echo 'check this job webhook configuration in settings of repo for referance'
      }
    }  
  }
}
